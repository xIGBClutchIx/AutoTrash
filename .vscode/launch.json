{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Hytale Server",
      "type": "java",
      "request": "launch",
      "mainClass": "org.gradle.wrapper.GradleWrapperMain",
      "classPaths": [
        "${workspaceFolder}/gradle/wrapper/gradle-wrapper.jar"
      ],
      "args": "runHytale",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "noDebug": true
    }
  ]
}
